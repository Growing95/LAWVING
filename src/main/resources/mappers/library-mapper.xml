	<select id="librarycount" resultType="int">
		select count(*) from library
	</select>
	<select id="librarylist" parameterType="Map" resultType="LibraryVo">
		select * from (
			select rownum r_num, a.* from (
				select * from library order by library_idx desc
			) a
		) where r_num between #{begin} and #{end}
	</select>
	<select id="blackcount" resultType="int">
		select count(*) from limit where limit_count = 5
	</select>
	<select id="searchLibraryList" parameterType="Map" resultType="LibraryVo">
		select * from (
			select rownum r_num, a.* from (
				select * from library order by library_idx desc
			) a
		) where r_num between #{begin} and #{end}
	</select>
